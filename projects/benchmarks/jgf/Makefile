all:
	(cd section1 && $(MAKE) all)
	(cd section2 && $(MAKE) all)
	(cd section3 && $(MAKE) all)

progs:
	(cd section1 && $(MAKE) progs)
	(cd section2 && $(MAKE) progs)
	(cd section3 && $(MAKE) progs)

runs:
	(cd section1 && $(MAKE) runs)
	(cd section2 && $(MAKE) runs)
	(cd section3 && $(MAKE) runs)

empty:
	(cd section1 && $(MAKE) empty)
	(cd section2 && $(MAKE) empty)
	(cd section3 && $(MAKE) empty)

clean:
	(cd section1 && $(MAKE) clean)
	(cd section2 && $(MAKE) clean)
	(cd section3 && $(MAKE) clean)
